	Title = {WEKA Machine Learning Project: Cow Culling},
	Year = {1994}}

@inproceedings{Holmes1994,
	Abstract = {WEKA is a workbench for machine learning that is intended to
aid in the application of machine learning techniques to a variety of real-world
problems, in particular, those arising from agricultural and horticultural
domains. Unlike other machine learning projects, the emphasis is on providing a
working environment for the domain specialist rather than the machine learning
expert. Lessons learned include the necessity of providing a wealth of
interactive tools for data manipulation, result visualization, database linkage,
and cross-validation and comparison of rule sets, to complement the basic
machine learning tools.},
	Address = {Brisbane, Australia},
	Author = {Holmes, G. and Donkin, A. and Witten, I.H.},
	Booktitle = {Proc Second Australia and New Zealand Conf. on Intelligent
Information Systems},
	Pdf =
