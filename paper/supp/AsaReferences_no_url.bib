	Title = {K*: An Instance-Based Learner Using an Entropic Distance
Measure},
	Year = {1995}}

@inproceedings{Cleary1995_3,
	Abstract = {The PPM data compression scheme has set the performance
standard in lossless compression of text throughout the past decade. The
original algorithm was first published in 1984 by Cleary and Witten [3], and a
series of improvements was described by Moffat [6], culminating in a careful
implementation, called PPMC, which has become the benchmark version. This still
achieves results superior to virtually all other compression methods, despite
many attempts to better it. Other methods such as those based on Ziv-Lempel
coding [9] are more commonly used in practice, but their attractiveness lies in
their relative speed rather than any superiority in compression-indeed, their
compression performance generally falls distinctly below that of PPM in
practical benchmark tests [1].},
